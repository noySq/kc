import:
	org.apache.commons.io.FileUtils
	java.io.File

function delete_world(ID: text):

	set {_world} to get_island_world({_ID})

	if {_world} is not set:
		stop

	if {_world} is not loaded:
		stop

	unload world {_world}
	delete world file for {_world}

	FileUtils.deleteDirectory(new File("%{_world}%"))