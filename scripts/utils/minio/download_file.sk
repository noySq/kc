import:
	io.minio.DownloadObjectArgs

function download_file(bucketName: text, objectName: text, path: text):

	if bucket_exists({_bucketName}) = true:

		set {_args} to DownloadObjectArgs.builder().bucket({_bucketName}).object({_objectName}).filename({_path}).build()
		{minio::client}.downloadObject({_args})