import:
	io.minio.MinioClient

on load:
	set {_endpoint} to "http://192.168.0.0:9000"
	set {_username} to "username"
	set {_password} to "password"

	set {_builder} to MinioClient.builder()
	set {_builder} to {_builder}.endpoint({_endpoint})
	set {_builder} to {_builder}.credentials({_username}, {_password})
	set {minio::client} to {_builder}.build()